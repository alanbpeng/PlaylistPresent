<!DOCTYPE html>
<html>
<head>
  <title>PlaylistPresent</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:error] %>
  <div><%= flash[:error] %></div>
  <br />
<% end %>
<% if flash[:info] %>
  <div><%= flash[:info] %></div>
  <br />
<% end %>

<%= yield %>

</body>
</html>
